import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
from statsmodels.tsa.seasonal import seasonal_decompose
from statsmodels.tsa.stattools import adfuller, kpss
from statsmodels.tsa.holtwinters import ExponentialSmoothing
from statsmodels.tsa.arima.model import ARIMA
from sklearn.metrics import mean_squared_error, mean_absolute_error
import warnings
warnings.filterwarnings('ignore')

# Load the data
file_path = 'Electric_Production.csv'
data = pd.read_csv(file_path)

# Parse date and set as index
# Convert DATE column to datetime using the correct format
data.iloc[:, 0] = pd.to_datetime(data.iloc[:, 0], format='%m/%d/%Y')  # Changed format
data.set_index(data.columns[0], inplace=True)
data.columns = ['production']

# Exploratory Data Analysis
plt.figure(figsize=(12,6))
plt.plot(data.production, label='Electric Production')
plt.title('Electric Production Over Time')
plt.xlabel('Date')
plt.ylabel('Production')
plt.legend()
plt.show()

# Decompose time series to see trend, seasonality
decomposition = seasonal_decompose(data.production, model='multiplicative', period=12)
fig = decomposition.plot()
fig.set_size_inches(14,8)
plt.show()

# Stationarity check functions
def adf_test(series):
    result = adfuller(series)
    print(f'ADF Statistic: {result[0]}')
    print(f'p-value: {result[1]}')
    if result[1] <= 0.05:
        print('Series is stationary')
    else:
        print('Series is NOT stationary')

def kpss_test(series):
    statistic, p_value, _, _ = kpss(series, regression='c')
    print(f'KPSS Statistic: {statistic}')
    print(f'p-value: {p_value}')
    if p_value <= 0.05:
        print('Series is NOT stationary')
    else:
        print('Series is stationary')

# Check stationarity on original data
adf_test(data.production)
kpss_test(data.production)

# Difference the series if needed to make it stationary
data['production_diff'] = data.production.diff().dropna()
adf_test(data['production_diff'].dropna())
kpss_test(data['production_diff'].dropna())

# Split data into train and test (last 12 months as test)
train = data.iloc[:-12]
test = data.iloc[-12:]

# Model 1: Holt-Winters Exponential Smoothing
model_hw = ExponentialSmoothing(train.production, seasonal='mul', seasonal_periods=12).fit()
pred_hw = model_hw.forecast(steps=12)

# Model 2: ARIMA (orders to be tuned)
model_arima = ARIMA(train.production, order=(1,1,1), seasonal_order=(1,1,1,12)).fit()
pred_arima = model_arima.forecast(steps=12)

# Evaluate models
rmse_hw = np.sqrt(mean_squared_error(test.production, pred_hw))
mae_hw = mean_absolute_error(test.production, pred_hw)

rmse_arima = np.sqrt(mean_squared_error(test.production, pred_arima))
mae_arima = mean_absolute_error(test.production, pred_arima)

print(f'Holt-Winters RMSE: {rmse_hw:.2f}, MAE: {mae_hw:.2f}')
print(f'ARIMA RMSE: {rmse_arima:.2f}, MAE: {mae_arima:.2f}')

# Plot actual vs forecast
plt.figure(figsize=(12,6))
plt.plot(train.index, train.production, label='Train')
plt.plot(test.index, test.production, label='Test')
plt.plot(pred_hw.index, pred_hw, label='Holt-Winters Forecast')
plt.plot(pred_arima.index, pred_arima, label='ARIMA Forecast')
plt.title('Electric Production Forecasts')
plt.xlabel('Date')
plt.ylabel('Production')
plt.legend()
plt.show()
